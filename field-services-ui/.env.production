# Production environment variables for Heroku
# These will be overridden by Heroku config vars

# Work Order Service URL - will be set to the Heroku app URL
VITE_WORK_ORDER_SERVICE_URL=https://your-work-order-service-app.herokuapp.com/api/v1

# Other service URLs (for future use)
VITE_USER_SERVICE_URL=https://localhost:8081/api/v1
VITE_RESOURCE_SERVICE_URL=https://localhost:8083/api/v1

# App configuration
VITE_APP_NAME=Field Services Management
VITE_APP_VERSION=1.0.0
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false