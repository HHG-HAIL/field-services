# Environment Variables for Backend Integration
# Copy this to .env in the field-service-app directory for backend integration

# Backend API URLs
REACT_APP_API_BASE_URL=http://localhost:8081
REACT_APP_TECHNICIAN_API_URL=http://localhost:8082
REACT_APP_SCHEDULE_API_URL=http://localhost:8083

# WebSocket URL for real-time updates
REACT_APP_WS_URL=ws://localhost:8081/ws

# App Configuration
REACT_APP_MODE=production